{
    "compilerOptions": {
        "target": "ESNext",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "allowJs": true,
        "checkJs": false,
        "noEmit": true,
        "strict": true,
        "skipLibCheck": true,
        "isolatedModules": true,
        "esModuleInterop": true,
        "resolveJsonModule": true
    },
    "include": [
        ".vitepress/config.mts",
        "**/*.ts",
        "**/*.js",
        "**/*.vue"
    ],
    "exclude": [
        "node_modules",
        ".vitepress/dist",
        ".vitepress/cache"
    ]
}